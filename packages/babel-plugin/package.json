{
  "name": "@groqz/babel-plugin",
  "version": "0.0.1",
  "keywords": [
    "babel-plugin"
  ],
  "bugs": {
    "url": "https://github.com/stipsan/groqz/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/stipsan/groqz.git",
    "directory": "packages/babel-plugin"
  },
  "license": "MIT",
  "author": "Cody Olsen <stipsan@gmail.com>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "pkg build --strict",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "pkg watch",
    "lint": "TIMING=1 eslint .",
    "test": "NODE_ENV=test jest"
  },
  "browserslist": [
    "> 0.2% and supports es6-module and supports es6-module-dynamic-import and not dead and not IE 11",
    "maintained node versions"
  ],
  "jest": {
    "modulePathIgnorePatterns": [
      "/test/fixtures/"
    ],
    "testEnvironment": "node",
    "testRegex": "./test/.+\\.js$"
  },
  "dependencies": {
    "@babel/helper-plugin-utils": "^7.20.2",
    "@babel/types": "^7.19.4",
    "@groqz/sanity": "workspace:*"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/generator": "^7.20.7",
    "@babel/parser": "^7.19.4",
    "@groqz/tsconfig": "workspace:*",
    "@sanity/pkg-utils": "2.3.1",
    "@types/babel__core": "^7.1.20",
    "@types/babel__generator": "^7.6.4",
    "@types/babel__helper-plugin-utils": "^7.10.0",
    "@types/prettier": "^2.7.2",
    "babel-plugin-tester": "^10.1.0",
    "eslint": "^8.30.0",
    "eslint-config-groqz": "workspace:*",
    "groq": "^2.33.2",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "typescript": "^4.9.4",
    "vitest": "^0.26.2",
    "zod": "^3.20.2"
  },
  "peerDependencies": {
    "@babel/core": "^7",
    "groqz": "workspace:*",
    "typescript": "^4.9",
    "zod": "^3"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  }
}
