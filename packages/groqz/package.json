{
  "name": "groqz",
  "version": "1.0.4",
  "bugs": {
    "url": "https://github.com/stipsan/groqz/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/stipsan/groqz.git",
    "directory": "packages/groqz"
  },
  "license": "MIT",
  "author": "Cody Olsen <stipsan@gmail.com>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "source": "./src/cli/index.ts",
      "require": "./dist/cli.cjs",
      "default": "./dist/cli.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "bin": {
    "groqz": "./bin/groqz.cjs"
  },
  "files": [
    "bin",
    "dist",
    "CHANGELOG.md"
  ],
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "pkg build --strict",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "pkg watch",
    "lint": "TIMING=1 eslint .",
    "test": "vitest run"
  },
  "browserslist": [
    "> 0.2% and supports es6-module and supports es6-module-dynamic-import and not dead and not IE 11",
    "maintained node versions"
  ],
  "dependencies": {
    "@babel/parser": "^7.19.4",
    "@babel/traverse": "^7.19.4",
    "@babel/types": "^7.19.4",
    "@groqz/json": "workspace:*",
    "@groqz/sanity": "workspace:*",
    "@groqz/to-ts": "workspace:*",
    "chokidar": "^3.5.3",
    "commander": "^11.0.0",
    "prettier": "^2.8.1"
  },
  "devDependencies": {
    "@groqz/tsconfig": "workspace:*",
    "@sanity/pkg-utils": "2.0.4-beta.1",
    "@types/babel__traverse": "^7.18.3",
    "@types/minimatch": "^5.1.2",
    "@types/node": "^18.11.18",
    "eslint": "^8.30.0",
    "eslint-config-groqz": "workspace:*",
    "minimatch": "^5.1.2",
    "sanity": "^3.1.2",
    "typescript": "^4.9.4",
    "vitest": "^0.26.2",
    "zod": "^3.20.2"
  },
  "peerDependencies": {
    "sanity": "^3",
    "typescript": "^4.9",
    "zod": "^3"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  }
}
